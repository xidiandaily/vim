--_echo='import lgametestclientproto.ctags'
--langdef=lgametestclientproto
--map-lgametestclientproto=.lua
--kinddef-lgametestclientproto=m,macro,macro
--kinddef-lgametestclientproto=M,macrosgroup,macrosgroup
--kinddef-lgametestclientproto=s,struct,struct
