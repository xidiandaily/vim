--regex-lgametestclientproto=/[^<]+<macrosgroup[ \t]+[^>]*name=\"([^\"]+)\"[ \t]+[^>]*>.*$/\1/M/{scope=push}
--regex-lgametestclientproto=/<\/macrosgroup>///{scope=pop}{placeholder}
--regex-lgametestclientproto=/[^<]+<union[ \t]+[^>]*name=\"([^\"]+)\"[ \t]+[^>]*>.*$/\1/u/{scope=push}
--regex-lgametestclientproto=/<\/union>///{scope=pop}{placeholder}
